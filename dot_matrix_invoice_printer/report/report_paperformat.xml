<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Custom paper format for pre-printed paper: 28cm height x 25cm width -->
    <record id="paperformat_dot_matrix_custom" model="report.paperformat">
        <field name="name">Dot Matrix Pre-printed Paper</field>
        <field name="default" eval="False"/>
        <field name="format">custom</field>
        <field name="page_height">280</field>  <!-- 28cm in mm -->
        <field name="page_width">250</field>   <!-- 25cm in mm -->
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">96</field>
    </record>
</odoo>